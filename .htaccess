# Options +FollowSymLinks
RewriteEngine ON

# #Custom 404 page redirect
ErrorDocument 404 https://themodernattire.com/404.html
DirectoryIndex index.php

# #Remove all php file extensions
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME}.php !-d
RewriteRule ^([^\.]+)$ $1.php [NC,L]

# RewriteRule ^index\.php$ / [R=301,L]

#e.g : products/Shop/1/ShararaNew  products/LatestCollection/1/prid
RewriteRule ^Shop/([^/]+)/([^/]+)/([^/]+)/([^/]+)$ ./singleprod.php?pg=$1&key=$2&prcode=$3&keywords=$4 [L]
